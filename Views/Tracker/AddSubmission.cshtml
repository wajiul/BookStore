@model Submission

<div class="container justify-content-center">
    <h3 class="text-center">Add new book</h3>
    <form method="post" asp-controller="Tracker" asp-action="AddSubmission">
        <div class="form-group">
            <label asp-for="handle"></label>
            <input class="form-control" asp-for="handle" />
        </div>

        <div class="form-group">
            <label asp-for="jsonData"></label>
            <input type="file" class="form-control" asp-for="jsonData" />
        </div>
        <div class="form-group">

            <input type="submit" value="Add book" class="btn btn-primary" />
        </div>
    </form>
</div>